page {
    background: #ffffff;
    padding: 0;
    margin: 0;
    box-sizing: border-box;
    height: 100%;
    overflow-y: hidden;
}

scroll-view {
    -webkit-overflow-scrolling: touch;
}

.cardFood {
    width: 100%;
    height: 100%;
    background: #ffffff;
    z-index: 666
}

.top {
    width: 100%;
