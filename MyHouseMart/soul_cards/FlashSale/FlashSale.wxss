    position: relative;
    top: 0;
    text-align: center;
}

.Yesterday {
    display: inline-block;
    position: relative;
    margin-top: 12rpx;
    margin-right: 128rpx;
}

.Yesterday:nth-child(3) {
    margin-right: 0;
}

.Yesterday .title {
    font-size: 32rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(255, 179, 179, 1);
    line-height: 44rpx;
