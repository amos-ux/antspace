    border-radius: 52rpx;
    margin-top: 4rpx;
}

.left {
    width: 164rpx;
    height: 92%;

    position: relative;
    /* margin-top: 24rpx; */
    display: inline-block;
    background: rgba(245, 245, 245, 1);
    box-sizing: border-box;
    /* border-right: 1rpx solid rgba(230,230,230,1); */
    z-index: 10;
}

.left_item {
    width: 164rpx;
    height: 136rpx;
    display: inline-block;
    background: rgba(245, 245, 245, 1);
    position: relative;
    box-sizing: border-box;
    border-right: 1rpx solid rgba(230, 230, 230, 1);
}

.left_item:nth-child(1) {
    width: 164rpx;
    height: 136rpx;
    background: rgba(245, 245, 245, 1);
    position: relative;
    box-sizing: border-box;
    display: inline-block;
    border-top: 1rpx solid rgba(230, 230, 230, 1);
}

.left_item_click {
    width: 164rpx;
    height: 136rpx;
    position: relative;
    box-sizing: border-box;
    background: white;
    display: inline-block;
    border-right: 1rpx solid white;
    border-top: 1rpx solid rgba(230, 230, 230, 1);
    border-bottom: 1rpx solid rgba(230, 230, 230, 1);
    z-index: 50;
}

.left_item_click:nth-child(1) {
    border-top: 0;
    display: inline-block;
}

.top_bottom {
    position: relative;
    bottom: 0rpx;
    width: 702rpx;
    height: 80rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 16rpx 16rpx 0px 0px;
    margin: 0 auto;
    margin-top: 10rpx;
}

.top_bottom .status {
    width: 660rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    margin: 0 auto;
    position: relative;
}

.top_bottom .status::before {
    position: absolute;
    left: 0;
    bottom: 0;
    content: " ";
    width: 100%;
    height: 1rpx;
    border-bottom: 1rpx solid #EBEBEB;
}

.top_bottom .status .status_yes {
    font-size: 26rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(43, 45, 51, 1);
}

.daty_end {
    font-size: 26rpx;
}

.status_date {
    display: inline-block;
