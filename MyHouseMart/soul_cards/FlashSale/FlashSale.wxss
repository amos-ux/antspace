    background: rgba(255, 90, 82, 1);
    border-radius: 4rpx;
    color: #FFFFFF;
    line-height: 36rpx;
    text-align: center;
    font-size: 28rpx;
}

.daty_end:last-child {
    margin-left: 16rpx;
}

.daty_end:nth-child(1) {
    margin-right: 16rpx;
}

.status_other {
    color: #FF5A52;
    margin-left: 4rpx;
    margin-right: 6rpx;
}


