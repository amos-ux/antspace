  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  border-bottom: 1rpx solid #ebebeb;
}

.list:first-child {
  height: 224rpx;
}

.list:last-child::after {
  border-bottom: 0;
}

.list:nth-child(1) .left {
