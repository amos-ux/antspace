}
.down .images{
    width:30rpx;
    height:18rpx;
    animation: xuanzhuan 0.1s;
    animation-fill-mode:forwards;
}
@keyframes xuanzhuan{
    0%{
        transform:rotate(18deg);
    }
    20%{
        transform:rotate(36deg);
    }
    40%{
        transform:rotate(72deg);
    }
    60%{
        transform:rotate(108deg);
    }
    80%{
        transform:rotate(144deg);
    }
    100%{
        transform:rotate(180deg);
    }
}
