page {
  background: #fff;
  width: 100%;
  overflow: hidden;
}

.page {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 140rpx;
  overflow-x: hidden;
  border-top: 1rpx solid #E6E6E6;
  box-sizing: border-box;
}

.manage {
  position: relative;
  background: #fff;
  width: 100%;
  height: 160rpx;
  padding: 36rpx;
  display: flex;
  justify-content: flex-start;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  border-bottom: 1rpx solid #E6E6E6;
}

.phone-mange {
  height: 36rpx;
  width: 72rpx;
  line-height: 36rpx;
  background: #FFEB0D;
  text-align: center;
  color: #595524;
  font-size: 22rpx;
  border-radius: 16rpx;
  margin-left: 12rpx;
  font-weight: 500;
}

.phone-home,.phone-homa {
  width: 72rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  color: #FFA333;
  background-color: #FFF2D9;
  border-radius: 16rpx;
  font-size: 22rpx;
  margin-left: 12rpx;
  font-weight: 500;
}

.phone-name {
  font-size: 13px;
  color: #63646b;
}

.phone-number {
  font-size: 13px;
  color: #63646b;
  margin-left: 12rpx;
}

.manage-view {
  font-size: 32rpx;
  width: 100%;
  color: #2B2D33;
  font-weight: 500;
}

.phone {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-top: 10rpx;
  font-weight: 400;
}

.toke {
  background: #fff;
  width: 100%;
  height: 132rpx;
  display: flex;
  justify-content: center;
  position: fixed;
  bottom: 0;
  z-index: 100;
}

.toke text {
  width: 500rpx;
  height: 92rpx;
  background: #fff;
  font-size: 16px;
  font-weight: 500;
  font-family:PingFangSC-Medium,PingFangSC;
  line-height: 92rpx;
  text-align: center;
  border-radius: 48rpx;
  box-shadow:0px 4px 17px 0px rgba(255,231,13,0.35);
  background:rgba(255,235,13,1);
}

.amend {
  position: absolute;
  font-size: 12px;
  top: 28px;
  right: 60px;
}

.length {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.view-location{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.location-img {
  width: 154rpx;
  height: 300rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.location-img image {
  width: 100%;
  height: 100%;
}

.location {
  text-align: center;
  font-size: 32rpx;
  color: #5c5b5a;
  margin-top: 24rpx;
}

.manage .editor{
  position: absolute;
  right: 32rpx;
  top: 64rpx;
  width: 36rpx;
  height: 36rpx;
}