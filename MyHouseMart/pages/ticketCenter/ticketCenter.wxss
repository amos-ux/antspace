page {
  background: #f5f6f7;
  width: 100%;
  height: 100%;
}

.ticketCenter {
  width: 100%;
  background: #f5f6f7;
  padding: 86rpx 32rpx 20rpx 32rpx;
  box-sizing: border-box;
}

.discount {
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  border-radius: 12rpx;
  width: 100%;
  padding: 24rpx 24rpx 18rpx 24rpx;
  box-sizing: border-box;
  margin-top: 24rpx;
}

.ticket {
  background: linear-gradient(to right, #ffee2e, #ffd814);
  border-radius: 6px;
  width: 100%;
  height: 103px;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.ticketMessage {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.ticketName {
  color: #63646b;
  font-size: 18px;
  font-weight: 500;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.ticketMoney text:nth-child(1) {
  color: #63646b;
  font-size: 16px;
  font-weight: 500;
}

.ticketMoney text:nth-child(2) {
  color: #63646b;
  font-size: 24px;
  font-weight: 500;
}

.discountTicket text:first-child, .discountBecoming text:nth-child(2) {
  color: #63646b;
  font-size: 48rpx;
  font-weight: 500;
}

.discountTicket text:nth-child(2), .discountBecoming text:first-child {
  color: #63646b;
  font-size: 32rpx;
  font-weight: 500;
}

.ticketCondition>text {
  color: #63646b;
  font-size: 12px;
}

.ticketDay {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.ticketDay text:nth-child(1) {
  color: #63646b;
  font-size: 12px;
}

.ticketDay text:nth-child(2) {
  color: #63646b;
  font-size: 12px;
}

.btnBox {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.ticketBox{
  display: flex;
  width: 100%;
  height: 56rpx;
  margin-top: 18rpx;
  position: relative;
}

.ticketBox .ticketBtn{
  background: #ff7459;
  border-radius: 28rpx;
  color: #fff;
  font-size: 13px;
  text-align: center;
  height: 56rpx;
  line-height: 56rpx;
  padding: 0 24rpx;
  position: absolute;
  right: 0;
}

.getBtn {
  width: 152rpx;
  height: 28px;
  background: #ff7459;
  border-radius: 14px;
  color: #fff;
  font-size: 13px;
  text-align: center;
  line-height: 28px;
  margin: 0;
  box-sizing: content-box;
  padding: 0;
  margin-top: 18rpx;
}

.pop {
  position: fixed;
  bottom: 0;
  top: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.2);
  display: flex;
  justify-content: center;
  align-items: center;
}

.pop-up {
  width: 580rpx;
  min-height: 305px;
  background: #fff;
  border-radius: 5px 5px;
}

.pop-up>view {
  width: 100%;
  height: 70px;
  text-align: center;
  line-height: 70px;
  color: #292928;
  font-size: 18px;
}

.scroll {
  width: 100%;
  height: 335px;
  padding: 0 46rpx 0 36rpx;
  box-sizing: border-box;
}

.pop-img {
  width: 80rpx;
  height: 40px;
  position: fixed;
  bottom: 40px;
  left: 336rpx;
}

.pop-img>image {
  width: 80rpx;
  height: 40px;
}

.scrollText {
  color: #5c5b5a;
  font-size: 15px;
  text-overflow: ellipsis;
  word-wrap: break-word;
}

.goodsTicket {
  width: 100%;
  height: 114px;
  border-radius: 4px;
  padding: 16px 24rpx 12px 34rpx;
  box-sizing: border-box;
  background: #fff;
  margin-top: 12px;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.goodsTicketMessage {
  display: flex;
  flex-direction: row;
}

.goodsTicketImg {
  width: 104rpx;
  height: 52px;
}

.goodsTicketRule {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 78%;
}

.goodsTicketName {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.goodsTicketName text:nth-child(1) {
  color: #2b2d33;
  font-size: 18px;
  font-weight: 500;
  width: 350rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.goodsTicketName text:nth-child(2) {
  color: #5c5b5a;
  font-size: 12px;
}

.ticketNum {
  display: flex;
  flex-direction: row;
  color: #63646b;
  font-size: 12px;
  justify-content: space-between;
  align-items: center;
  margin-top: 10px;
}

.ticketNum text:nth-child(1) {
  width: 230rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.ticketTime {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-top: 15px;
}

.ticketTime>text,.ticketBox>text{
  color: #a3a5ad;
  font-size: 12px;
}

.allVouchers {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.allVouchers>image {
  width: 366rpx;
  height: 150px;
}

.allVouchers>text {
  color: #63646b;
  font-size: 16px;
}

.selectBar {
  width: 100%;
  height: 86rpx;
  line-height: 86rpx;
  display: flex;
  justify-content: center;
  align-content: center;
  background-color: #fff;
  border-top: 2rpx solid #f5f6f7;
  box-sizing: border-box;
  position: fixed;
  z-index: 999;
}

.selectBar .myCard {
  font-size: 30rpx;
  font-weight: 500;
  flex: 1;
  display: inline-block;
  text-align: center;
  color: #2b2d33;
}

.selectBar .pending {
  font-size: 30rpx;
  font-weight: 500;
  flex: 1;
  display: inline-block;
  text-align: center;
  color: #9b9c9e;
}

.underLine {
  width: 64rpx;
  height: 8rpx;
  background: #ffa333;
  position: absolute;
  bottom: 0;
  left: 152rpx;
  border-radius: 6rpx;
  z-index: 998;
}
