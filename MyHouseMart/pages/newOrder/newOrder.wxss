  height: 56rpx;
  border: 1rpx solid #e0e0e0;
  border-radius: 8rpx;
  color: #2b2d33;
  font-size: 26rpx;
  line-height: 56rpx;
  text-align: center;
  margin-right: 32rpx;
}

.order-yard {
  color: #ffa333;
  font-size: 30rpx;
