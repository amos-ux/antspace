.page-nav {
  width: 100%;
  height: 86rpx;
  z-index: 100;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  border-bottom: 1rpx solid #e6e6e6;
  box-sizing: border-box;
}

.page-all {
  color: #67686b;
  font-size: 28rpx;
