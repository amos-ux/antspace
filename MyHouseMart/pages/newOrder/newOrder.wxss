}

.number {
  position: absolute;
  top: -10rpx;
  right: -5rpx;
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  border-radius: 50%;
  background: rgba(255, 90, 82, 1);
  padding: 0 1rpx 0 1rpx;
  text-align: center;
  display: inline-block;
}

.label {
  width: 64rpx;
  height: 8rpx;
  background: rgba(255, 163, 51, 1);
  border-radius: 4rpx;
  position: absolute;
  bottom: -20rpx;
  left: 50%;
  margin-left: -32rpx;
}

.order {
  width: 100%;
  margin-top: 102rpx;
}

.order-list {
  width: 100%;
  background: #fff;
  margin-top: 20rpx;
  padding-bottom: 24rpx;
}

.order .order-list:last-child {
  margin-bottom: 20rpx;
}

.order-list-nav {
  width: 100%;
  height: 84rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #f0f0f0;
}

.order-list-nav-left {
  display: flex;
  align-items: center;
  margin-left: 32rpx;
}

.order-list-nav-left image {
  width: 30rpx;
  height: 28rpx;
}

.order-list-nav-left view {
  color: #2b2d33;
  font-size: 30rpx;
  margin-left: 12rpx;
}

.order-list-nav-right {
  color: #a0a1a3;
  font-size: 30rpx;
  margin-right: 32rpx;
}

.order-content {
  width: 100%;
  /* height: 204rpx; */
}

.order-content-top {
  width: 100%;
  height: 160rpx;
  display: flex;
  align-items: center;
  position: relative;
}

.order-contet-img {
  width: 96rpx;
  height: 96rpx;
  margin-left: 32rpx;
  border-radius: 4rpx;
}

.order-contet-img image {
  width: 100%;
  height: 100%;
  border-radius: 4rpx;
}

.order-goods {
  width: 520rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #2b2d33;
  font-size: 28rpx;
  margin-left: 24rpx;
  font-weight: 500;
}

.order-goods::after {
  content: '';
  display: inline-block;
  width: 14rpx;
  height: 14rpx;
  border-top: 1px solid #a0a1a3;
  border-right: 1px solid #a0a1a3;
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  position: absolute;
  right: 34rpx;
  top: 50%;
  margin-top: -7rpx;
}

.order-contetn-bottom {
  height: 92rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-top: 1rpx solid #f0f0f0;
}

.order-time {
  color: #a0a1a3;
  font-size: 26rpx;
  margin-left: 32rpx;
}

.order-messge {
  display: flex;
  align-items: center;
}

.order-message-quantity {
  color: #2b2d33;
  font-size: 26rpx;
  margin-right: 24rpx;
}

.order-message-price {
  color: #2b2d33;
  font-size: 26rpx;
  margin-right: 32rpx;
}

.order-button {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.order-cancel {
  width: 148rpx;
  height: 56rpx;
  border: 1rpx solid #e0e0e0;
  border-radius: 8rpx;
  color: #2b2d33;
  font-size: 26rpx;
  line-height: 56rpx;
  text-align: center;
  margin-right: 20rpx;
}

.order-confirm {
  width: 148rpx;
  height: 56rpx;
