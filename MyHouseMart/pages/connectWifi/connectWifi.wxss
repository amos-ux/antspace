page {
  display: flex;
  height: 100%;
  justify-content: center;
  align-items: center;
}

.page {
  width: 100px;
  height: 40px;
  border-radius: 12rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: orange;
}

.user {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 2;
}

.user image {
  width: 100%;
  height: 100%;
}
