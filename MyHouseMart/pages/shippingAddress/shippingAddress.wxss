page {
  background: #fff;
  box-sizing: border-box;
  height: 100%;
}

.coverage {
  width: 100%;
  border-top: 1px solid #d8d8d8;
  height: 1px;
  box-sizing: border-box;
  position: fixed;
  top: 0;
}

.shippingAddress {
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  border-top: 1rpx solid #E6E6E6;
}

.addressItem {
  width: 100%;
  height: 80px;
  background: #fff;
  padding: 36rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #E6E6E6;
}

.shippingAddress .addressItem:last-of-type{
  border-bottom: none;
}

.addressMessage {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.addressDetail {
  color: #2b2d33;
  font-size: 16px;
  font-weight: 500;
}

.addressName {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 10rpx;
}

.adddefault {
  background: #FFEB0D;
  width: 72rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  border-radius: 16rpx;
  color: #595524;
  font-size: 22rpx;
  font-weight: 500;
  margin-right: 12rpx;
}

.addresidence {
  background: #fff;
  width: 72rpx;
  height: 32rpx;
  color: #ffa333;
  font-size: 22rpx;
  font-weight: 500;
  text-align: center;
  line-height: 32rpx;
  border-radius: 16rpx;
  background-color: #FFF2D9;
}

.addcompany {
  background: #fff;
  width: 72rpx;
  height: 32rpx;
  color: #FFA333;
  font-size: 22rpx;
  font-weight: 500;
  text-align: center;
  line-height: 32rpx;
  border-radius: 16rpx;
  background-color: #FFF2D9;
}

.adduserName {
  color: #63646b;
  font-size: 13px;
  margin-right: 12rpx;
}

.adduserNumber {
  color: #63646b;
  font-size: 26rpx;
  margin-right: 12rpx;
}

.addressImg {
  width: 36rpx;
  height: 36rpx;
}

.unusable {
  width: 100%;
  overflow-y: scroll;
  margin-bottom: 170rpx;
  box-sizing: border-box;
  background: #f5f6f7;
}

.BeyondDistribution {
  width: 100%;
  font-size: 32rpx;
  font-weight: 500;
  box-sizing: border-box;
  background-color: #FFF8EB;
  color: #FFA333;
  padding: 20rpx 36rpx;
  display: flex;
  flex-direction: column;
}

.BeyondDistribution text:first-child{
  font-weight: 500;
  color: #FFA333;
  font-size: 32rpx;
}

.BeyondDistribution text:nth-child(2){
  color: #FFA333;
  font-size: 24rpx;
  font-weight: 400;
}

/* 不可用地址 */

.locationItem {
  width: 100%;
  height: 80px;
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 32rpx 0 32rpx 32rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #E6E6E6;
}

.locationDetail {
  color: #C3C4C7;
  font-size: 16px;
  font-weight: 500;
}

.locationName {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 10rpx;
  flex-wrap: nowrap;
  overflow: hidden;
}

.default {
  background: #a3a5ad;
  width: 72rpx;
  height: 32rpx;
  color: #fff;
  font-size: 11px;
  font-weight: 500;
  text-align: center;
  line-height: 32rpx;
  margin-right: 12rpx;
  border-radius: 16rpx;
}

.residence {
  background: #fff;
  width: 72rpx;
  height: 32rpx;
  color: #C3C4C7;
  font-size: 22rpx;
  font-weight: 500;
  text-align: center;
  line-height: 32rpx;
  border-radius: 16rpx;
  background-color: #F5F5F5;
}

.company {
  width: 72rpx;
  height: 32rpx;
  color: #C3C4C7;
  font-size: 11px;
  font-weight: 500;
  text-align: center;
  line-height: 32rpx;
  border-radius: 16rpx;
  background-color: #F5F5F5;
}

.userName {
  color: #a3a5ad;
  font-size: 26rpx;
  margin-right: 12rpx;
  white-space: nowrap;
}

.userNumber {
  color: #a3a5ad;
  font-size: 26rpx;
  margin-right: 12rpx;
}

/* 添加地址 */
.addLocation {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
}

.exist-address{
  width: 100%;
  display: flex;
  height: 170rpx;
  justify-content: center;
  align-items: center;
  background: #fff;
  position: fixed;
  bottom: 0;
}

.addLocation>button{
  background: #ffeb0d;
  width: 360rpx;
  height: 84rpx;
  text-align: center;
  line-height: 84rpx;
  color: #2b2d33;
  font-size: 32rpx;
  font-weight: 500;
  box-shadow: 0px 4px 17px 0px rgba(255, 231, 13, 0.35);
  border-radius: 42rpx;
}

.exist-address>button{
  background: #ffeb0d;
  width: 500rpx;
  height: 92rpx;
  text-align: center;
  line-height: 92rpx;
  color: #2b2d33;
  font-size: 32rpx;
  font-weight: 500;
  box-shadow: 0px 4px 17px 0px rgba(255, 231, 13, 0.35);
  border-radius: 48rpx;
}

.manipulate {
  width: 100%;
  height: 57px;
}

.locationImg {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 100%;
}

.locationImg>image {
  width: 154rpx;
  height: 300rpx;
}

.locationImg>text {
  color: #5c5b5a;
  font-size: 32rpx;
  margin: 24rpx 0;
}
