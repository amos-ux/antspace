.temponent {
  width: 100%;
}

.temponent-img {
  width: 100%;
  border-radius: 12rpx;
  height: 138rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.title {
  color: #fff;
  font-size: 32rpx;
  margin-left: 46rpx;
}

.subtitle {
  width: 260rpx;
  height: 72rpx;
  color: #437107;
  font-size: 26rpx;
  margin-left: 78rpx;
}

.temponent-img view {
  width: 702rpx;
  border-radius: 12rpx;
  height: 104rpx;
  display: flex;
  align-items: center;
}

.contents {
  width: 702rpx;
  background: #f5f6f7;
  position: relative;
  margin: 0 auto;
  margin-top: 16rpx;
  margin-bottom: 24rpx;
}

.content_item:last-child {
  margin-bottom: 8rpx;
}

.good-discount {
  height: 24rpx;
}

.discount {
  height: 24rpx;
  margin-left: 16rpx;
  border: 2rpx solid #ff5a52;
  font-size: 20rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 90, 82, 1);
  line-height: 24rpx;
  text-align: center;
  border-radius: 14rpx;
  padding: 0 6rpx;
}

.morning {
  margin-left: 8rpx;
  height: 24rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  border: 2rpx solid #ffa333;
  border-radius: 14rpx;
  text-align: center;
  padding: 0 6rpx;
  color: rgba(255, 163, 51, 1);
  line-height: 24rpx;
}

.goods-price {
  margin-top: 14rpx;
  display: flex;
  justify-content: flex-start;
}

.content_item {
  display: inline-block;
  width: 232rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 8rpx;
  position: relative;
  margin-left: 8rpx;
  margin-top: 8rpx;
}

.content_item_top {
  width: 200rpx;
  height: 200rpx;
  margin: 0 16rpx 0 16rpx;
  position: relative;
}

.content_item_top image {
  display: block;
  width: 200rpx;
  height: 200rpx;
  margin-top: 16rpx;
  position: relative;
  border-radius: 8rpx;
}

.content_item_top .text {
  position: absolute;
  top: 0;
  left: 0;
  display: inline-block;
  padding: 0rpx 12rpx 0rpx 12rpx;
  background: linear-gradient(270deg, rgba(255, 87, 71, 1) 0%, rgba(255, 133, 51, 1) 100%);
  border-radius: 8rpx 0px 16rpx 0px;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.item_title {
  width: 200rpx;
  height: 76rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(43, 45, 51, 1);
  margin: 12rpx 16rpx 0 16rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.Discount {
  position: relative;
  /* width:64rpx; *//* height:32rpx; */
  font-size: 20rpx;
  text-align: center;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(255, 90, 82, 1);
  border: 1px solid rgba(255, 90, 82, 1);
  /* line-height:32rpx; */
  margin-left: 16rpx;
  margin-top: 14rpx;
  border-radius: 47rpx;
  display: inline-block;
  padding: 0rpx 8rpx 0rpx 8rpx;
}

.box-Beforediscount {
  height: 40rpx;
  margin-left: 16rpx;
  margin-top: 12rpx;
}

.Beforediscount {
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #ff5a52;
  /* margin:0 16rpx 0 16rpx; *//* line-height:34rpx; *//* margin-top: 10rpx; */
}

.text1 {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 40rpx;
  background: linear-gradient(270deg, rgba(255, 90, 82, 1) 0%, rgba(255, 125, 82, 1) 100%);
  color: #fff;
  left: 0;
  right: 0;
  font-size: 22rpx;
  text-align: center;
  line-height: 40rpx;
  z-index: 11;
}

.Afterdiscount {
  font-size: 14px;
  font-family: PingFangSC-Medium;
  margin: 12rpx 54rpx 0 16rpx;
  font-weight: 500;
  color: rgba(255, 90, 82, 1);
  line-height: 40rpx;
  display: inline-block;
  position: relative;
  top: -10rpx;
}

.complete {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.complete view {
  width: 120rpx;
  height: 120rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  color: rgba(255, 255, 255, 1);
  background: rgba(0, 0, 0, 0.6);
  font-size: 26rpx;
}

.add {
  display: inline-block;
  position: absolute;
  right: 16rpx;
  bottom: 16rpx;
  width: 40rpx;
  height: 40rpx;
  background: rgb(255, 235, 13, 1);
  border-radius: 50%;
  text-align: center;
  line-height: 40rpx;
  margin-top: 10rpx;
  margin-right: 15rpx;
}

.add image {
  width: 100%;
  height: 100%;
}

.good_box {
  width: 14px;
  height: 14px;
  position: fixed;
  border-radius: 50%;
  overflow: hidden;
  left: 50%;
  top: 50%;
  z-index: +99;
  background: red;
}

.temponent-content{
  margin-top:-8rpx;
  margin-left:16rpx;
}

.temponent-content .content_item:nth-child(3n+1){
  margin-left: 4rpx;
}