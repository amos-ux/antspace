.page{
  position: fixed;
  bottom: 0;
  left:0;
  right: 0;
  top: 0;
  background: rgba(0, 0, 0, .5);
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 888
}

.window{
  width: 648rpx;
  background: #fff;
  border-radius: 16rpx
}
.window-top{
  width: 100%;
  height: 102rpx;
  text-align: center;
  color: rgba(43, 45, 51, 1);
  font-size: 34rpx;
  padding-top: 40rpx;
  position: relative
}
.window-fixed{
  width: 196rpx;
  height: 16rpx;
  background: #FEDE0D;
  border-radius: 10rpx;
  position: absolute;
  top: 74rpx;
  left: 50%;
  margin-left: -98rpx;
  opacity: .7
}
.window-content{
  width: 100%;
  height: 278rpx;
  overflow-y: scroll
}
.window-list{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center
}
.box{
  width: 584rpx;
  height: 100%;
 display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #EBEBEB
}
.window-user{
  display: flex;
  align-items: center;

}
.window-icon{
  width: 52rpx;
  height: 52rpx;
  border-radius: 50%;

}
.window-icon image{
  width: 100%;
  height: 100%;
  border-radius: 50%
}
.window-name{
  color: rgba(43, 45, 51, 1);
  font-size: 28rpx;
  margin-left: 16rpx
}
.window-text{
  display: flex;
  align-items: center;
  color: rgba(43, 45, 51, 1);
  font-size: 28rpx;

}
.window-text text{
  color: #FFA333;
  font-size: 28rpx
}
.window-bottom{
  width: 100%;
  height: 128rpx;
  display: flex;
  justify-content: center
}
.window-button{
  width:220px;
  height:44px;
  background:linear-gradient(90deg,rgba(255,236,25,1) 0%,rgba(255,223,13,1) 100%);
  box-shadow:0px 4px 6px 2px rgba(235,209,0,0.25);
  border-radius:22px;
  font-size:16px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(43,45,51,1);
}