.temponent {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.temponent-img {
  width: 100%;
  height: 138rpx;
  border-radius: 12rpx;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

.title {
  position: absolute;
  top: 42rpx;
  left: 30rpx;
  color: #fff;
  font-size: 32rpx;
  margin-left: 46rpx;
}

.subtitle {
  position: absolute;
  top: 30rpx;
  right: 176rpx;
  width: 260rpx;
  height: 72rpx;
  color: #437107;
  font-size: 26rpx;
  margin-left: 78rpx;
}

.temponent-img view {
  width: 702rpx;
  border-radius: 12rpx;
  display: flex;
  align-items: center;
  height: 104rpx;
}

.goods {
  position: relative;
  width: 702rpx;
  height: 268rpx;
  border-bottom: 1rpx solid #ebebeb;
  background: #fff;
  border-radius: 16rpx;
  display: flex;
}

.goods-img {
  width: 200rpx;
  height: 200rpx;
  margin-top: 24rpx;
  margin-left: 20rpx;
  border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
  position: relative;
}
/* 多倍返现 */
.returnCash{
  position: absolute;
  bottom: 0;
  width: 100%;
  display: flex;

}
.return-view{
  flex-grow: 1;
  height: 22rpx;
  line-height: 22rpx;
  color:#fff;
  font-size: 16rpx;
  text-align: center;
  align-self:flex-end;
  background:linear-gradient(318deg,rgba(255,37,0,1) 0%,rgba(255,129,0,1) 69%,rgba(255,110,0,1) 100%,rgba(255,129,0,1) 100%);
}
.return-view1{
  height: 34rpx;
  line-height: 34rpx;
  padding: 0 4rpx 0 6rpx;
  font-size: 24rpx;
  background: #6D2CF2;
  box-shadow:0px 0px 4rpx 0px rgba(214,4,4,0.24);
  border-radius:12rpx 0px 0px 0px;
  color: #fff;
  font-family: "Alternate"
}
.goods-img image {
  width: 100%;
  height: 100%;
  border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
}

.goods-text {
  width: 482rpx;
}

.text1 {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 40rpx;
  background: linear-gradient(270deg, rgba(255, 90, 82, 1) 0%, rgba(255, 125, 82, 1) 100%);
  color: #fff;
  left: 0;
  right: 0;
  font-size: 22rpx;
  text-align: center;
  line-height: 40rpx;
  z-index: 11;
}

.text {
  position: absolute;
  top: 0;
  left: 0;
  display: inline-block;
  padding: 0rpx 12rpx 0rpx 12rpx;
  background: linear-gradient(270deg, rgba(255, 87, 71, 1) 0%, rgba(255, 133, 51, 1) 100%);
  border-radius: 8rpx 0px 16rpx 0px;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.goods-name {
  width: 440rpx;
  /* height: 88rpx; */
  margin-left: 20rpx;
  margin-top: 24rpx;
  color: #2b2d33;
  font-size: 30rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.discount {
  height: 24rpx;
  margin-left: 20rpx;
  border: 1rpx solid #ff5a52;
  font-size: 20rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 90, 82, 1);
  line-height: 24rpx;
  text-align: center;
  border-radius: 14rpx;
  padding: 0 6rpx;
}

.morning {
  margin-left: 8rpx;
  height: 24rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  border: 1rpx solid #ffa333;
  border-radius: 14rpx;
  text-align: center;
  padding: 0 6rpx;
  color: rgba(255, 163, 51, 1);
  line-height: 24rpx;
}

.goods-price {
  margin-top: 12rpx;
  display: flex;
  justify-content: flex-start;
}

.vipprice {
  margin-left: 20rpx;
  margin-top: 12rpx;
  color: #ff5a52;
  font-size: 34rpx;
  font-weight: 500;
}

.complete {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.complete view {
  width: 120rpx;
  height: 120rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  color: rgba(255, 255, 255, 1);
  background: rgba(0, 0, 0, 0.6);
  font-size: 26rpx;
}

.saleprice {
  margin-left: 20rpx;
  display: flex;
  justify-content: space-between;
}

.price {
  color: #a0a1a3;
  font-size: 24rpx;
}

.add {
  position: absolute;
  bottom: 24rpx;
  right: 24rpx;
  width: 56rpx;
  height: 56rpx;
}

.add image {
  width: 100%;
  height: 100%;
}

.good_box {
  width: 14px;
  height: 14px;
  position: fixed;
  border-radius: 50%;
  overflow: hidden;
  left: 50%;
  top: 50%;
  z-index: +99;
  background: red;
}
