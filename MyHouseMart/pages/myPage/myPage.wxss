  font-size: 26rpx;
  font-weight: 400;
  padding-right: 20rpx;
  height: 56rpx;
  line-height: 56rpx;
  position: relative;
}

.balance {
  width: 100%;
  padding: 10rpx 46rpx 54rpx 46rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
