@import '/myfont.wxss';

page {
  height: 100%;
  position: relative;
}

.page {
  width: 100%;
  height: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
}

.mylogin {
  width: 354rpx;
  height: 116rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 24rpx;
}

.mylogin view {
  width: 172rpx;
  height: 60rpx;
  border-radius: 30rpx;
  text-align: center;
  line-height: 60rpx;
  border: 2rpx solid #2b2d33;
  color: #2b2d33;
  font-size: 28rpx;
}

.pages {
  width: 100%;
  position: relative;
  height: 318rpx;
  background-color: #ffeb0d;
  z-index: 5;
  border: none;
}

.myview {
  width: 100%;
  height: 100%;
  display: flex;
  padding: 0 24rpx;
  box-sizing: border-box;
}

.myimg {
  width: 116rpx;
  height: 116rpx;
  border: 6rpx solid rgba(255, 255, 255, 0.2);
  border-radius: 50%;
  background: #fff;
}

.myimg image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.mytext {
  height: 116rpx;
  display: flex;
  flex-direction: column;
  margin-left: 24rpx;
}

.myyard {
  width: 180rpx;
  height: 68rpx;
  background: #fff;
  display: flex;
  z-index: 111;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  border-radius: 34rpx;
  box-shadow: 0rpx 24rpx 28rpx 0rpx rgba(206, 128, 10, 0.23);
  margin-top: 24rpx;
}

.myyard image {
  width: 100%;
  height: 100%;
}

.name {
  color: #2b2d33;
  font-size: 36rpx;
  font-weight: 500;
  width: 354rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.vip {
  height: 36rpx;
  line-height: 36rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  padding: 0 12rpx;
  align-items: center;
  background: #2b2d33;
  border-radius: 18rpx;
  position: relative;
  color: #f2cf9b;
}

.vip .general-img {
  width: 24rpx;
  height: 20rpx;
  vertical-align: middle;
  position: relative;
  z-index: 10;
}

.vip .vip-img {
  width: 70rpx;
  height: 70rpx;
  position: absolute;
  left: -18rpx;
  top: -18rpx;
}

.viptext {
  font-size: 22rpx;
}

.vipname {
  font-size: 22rpx;
  padding-left: 32rpx;
}

.date-list {
  color: #f2cf9b;
  font-size: 24rpx;
  line-height: 34rpx;
  margin-top: 6rpx;
  font-weight: 300;
}

.date-text {
  color: #f2cf9b;
  font-size: 24rpx;
  line-height: 34rpx;
  margin-left: 12rpx;
}

.date-list .date-text:first-child {
  margin-left: 0;
}

.flex {
  display: flex;
  margin-top: 10rpx;
}

.radian {
  width: 100%;
  flex: 1;
  background-color: #f5f6f7;
  z-index: 222;
  position: relative;
  margin-top: -30rpx;
}

.image {
  height: 140rpx;
  width: 706rpx;
  border: none;
  box-shadow: 0 24rpx 36rpx 8rpx rgba(43, 44, 51, 0.26);
  background: #24252e;
  background-size: cover;
  position: absolute;
  top: 150rpx;
  left: 24rpx;
  z-index: 10;
  border-top-left-radius: 32rpx;
  border-top-right-radius: 32rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.pages-img {
  height: 140rpx;
  width: 706rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}

.myicon {
  width: 100%;
  position: relative;
}

.myicon-top {
  height: 100%;
  display: inline-block;
  margin-left: 26rpx;
  margin-top: 26rpx;
  line-height: 30rpx;
}

.vip-card {
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 26rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
}

.vip-card-text {
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #f2cf9b;
  line-height: 36rpx;
}

.vip-fun {
  font-size: 24rpx;
  font-weight: 400;
  font-family: PingFangSC-Regular, PingFang SC;
  display: flex;
  padding: 0 60rpx;
  box-sizing: border-box;
  justify-content: space-between;
}

.vip-text {
  display: flex;
  align-items: flex-end;
}

.vip-text .vip-date {
  color: #f2cf9b;
  font-size: 24rpx;
  font-weight: 300;
}

.vip-text .vip-image {
  width: 40rpx;
  height: 40rpx;
}

.vip-date .vip-exipredate:nth-child(2) {
  margin-left: 16rpx;
  padding: 0 16rpx;
  box-sizing: border-box;
  border-left: 1rpx solid #a0a1a3;
  height: 28rpx;
  line-height: 28rpx;
  display: inline-block;
}

.vip-showtip {
  width: 100%;
  height: 48rpx;
  line-height: 48rpx;
  color: #a0a1a3;
  font-size: 24rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
}

.vip-btn {
  height: 56rpx;
  line-height: 56rpx;
  color: #f2cf9b;
  font-size: 26rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  padding: 0 26rpx;
}

.vip-btn::after {
  content: " ";
  display: inline-block;
  height: 12rpx;
  width: 12rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #f2cf9b;
  border-style: solid;
  transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -6rpx;
  right: 5%;
}

.myicon-text {
  color: #f2cf9b;
  font-size: 28rpx;
  display: inline-block;
  border-width: 500;
  /* margin-left: 16rpx; */
}

.atonce {
  width: 136rpx;
  height: 56rpx;
  margin-right: 34rpx;
  background: #f2cf9b;
  color: #2b2d33;
  font-size: 24rpx;
  font-weight: 500;
  display: inline-block;
  border-radius: 28rpx;
  text-align: center;
  line-height: 56rpx;
  right: 0;
  position: absolute;
  top: 50%;
  margin-top: -24rpx;
}

.card {
  box-sizing: border-box;
  margin: 14rpx 24rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  border-radius: 12rpx;
  background: #fff;
}

.card .card-cont {
  width: 100%;
  height: 290rpx;
  background: linear-gradient(90deg, rgba(38, 40, 51, 1) 0%, rgba(67, 70, 82, 1) 100%);
  border-radius: 12rpx;
  overflow: hidden;
}

.balance .slideup {
  position: absolute;
  bottom: 16rpx;
  right: 0;
  background-color: #464857;
  color: #fff;
  border-radius: 100rpx 0 0 100rpx;
  font-size: 26rpx;
  padding: 0 16rpx;
  height: 48rpx;
  line-height: 48rpx;
}

.slideup>image {
  width: 40rpx;
  height: 20rpx;
