.search-bar {
  background-color: #fff;
}

.shops {
  position: fixed;
  z-index: 100;
  left: 24rpx;
  bottom: 56rpx;
  width: 88rpx;
  border-radius: 50%;
  box-shadow: 0 2rpx 4rpx 0 rgba(0, 0, 0, 0.2);
  height: 88rpx;
  background: #fff;
}

.shops-view {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.shops-view text {
  width: 44rpx;
  height: 32rpx;
  text-align: center;
  line-height: 32rpx;
  background: rgba(255, 90, 82, 1);
  color: rgba(255, 255, 255, 1);
  border-radius: 16rpx;
  font-size: 20rpx;
  position: absolute;
  right: -4rpx;
  top: 6rpx;
}

.shops-view image {
  width: 46rpx;
  height: 46rpx;
}

.search-input {
  width: 100%;
  padding: 24rpx 24rpx;
  background-color: #fff;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}

.search-view {
  background: rgba(242, 242, 242, 1);
  width: 614rpx;
  height: 64rpx;
  border-radius: 32rpx;
  position: relative;
}

.search-tab {
  font-size: 28rpx;
  color: #2b2d33;
  flex: 1;
  border: none;
  height: 64rpx;
  line-height: 64rpx;
  width: 530rpx;
  position: relative;
  padding-left: 84rpx;
  box-sizing: border-box;
  z-index: 1;
  text-align: left;
}

.input-placeholder {
  color: #a0a1a3;
}

.input-text {
  display: inline-block;
  height: 64rpx;
  line-height: 64rpx;
}

.search-icon {
  width: 32rpx;
  position: absolute;
  top: 50%;
  left: 32rpx;
  transform: translateY(-50%);
}

.empty-icon {
  width: 40rpx;
  height: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 12rpx;
  right: 20rpx;
  z-index: 998;
}

.empty-icon image {
  width: 40rpx;
  height: 40rpx;
}

.search-list {
  width: 100%;
  box-sizing: border-box;
  border-top: 1rpx solid #ebebeb;
  padding-left: 32rpx;
  height: 100%;
}

.order-name {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #2b2d33;
  border-bottom: 1rpx solid #ebebeb;
}

.history {
  width: 100%;
  margin-top: 24rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
}

.history-top {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.history-top>text {
  font-size: 30rpx;
  color: #a0a1a3;
}

.history-top .del-icon {
  width: 32rpx;
  height: 34rpx;
}

.history-bottom {
  margin-top: 24rpx;
  color: #63646b;
  font-size: 30rpx;
}

.history-bottom .toResult {
  border-radius: 8rpx;
  background-color: #f7f7f7;
  display: inline-block;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  padding: 0 24rpx;
  height: 68rpx;
  line-height: 68rpx;
  min-width: 100rpx;
  text-align: center;
  max-width: 340rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.noshop {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  text-align: center;
}

.noshop>image {
  width: 100%;
}

.noshop>text {
  font-size: 32rpx;
  color: #2b2d33;
  font-weight: 500;
  margin-top: -80rpx;
}

.has-shop {
  width: 100%;
  min-height: 1300rpx;
  padding: 16rpx 24rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-content: flex-start;
  flex-wrap: wrap;
  background-color: #f5f6f7;
}

.shop-con {
  width: 344rpx;
  height: 528rpx;
  background-color: #fff;
  border-radius: 16rpx;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  margin-bottom: 16rpx;
  position: relative;
}

.shop-con .refundValue{
  min-width: 132rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius:16rpx 0px 16rpx 0px;
  background:linear-gradient(270deg,rgba(255,87,71,1) 0%,rgba(255,133,51,1) 100%);
  color: #fff;
  font-weight: 400;
  font-size: 24rpx;
  z-index: 2;
  position: absolute;
  top: 0;
  left: 0;
}

.shop-img{
  width: 344rpx;
  height: 344rpx;
  position: relative;
}

.shop-message{
  width: 100%;
  height: 40rpx;
  background-color: #FFF1E0;
  color: #FFA333;
  box-sizing: border-box;
  font-size: 22rpx;
  line-height: 40rpx;
  font-weight: 400;
  position: absolute;
  bottom: 0;
  left: 0;
}

.shop-message>text{
  padding-left: 16rpx;
}

.shop-img>image {
  width: 344rpx;
  height: 344rpx;
  border-radius: 16rpx 16rpx 0 0;
}

.shop-main {
  padding: 20rpx;
}

.shop-con .normal-img{
  width:  344rpx;
  height: 344rpx;
  border-radius:16rpx 16rpx 0px 0px;
}

.shop-main .shop-name {
  display: block;
  width: 100%;
  color: #2b2d33;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 12rpx;
}

.shop-main .discount {
  margin: 6px 0;
  font-size: 20rpx;
  height: 36rpx;
}

.shop-main .discount .satisfied {
  display: inline-block;
  font-size: 20rpx;
  color: #FF5A52;
  border-radius: 24rpx;
  margin-right: 8rpx;
  border: 1rpx solid #FF5A52;
  padding: 0 8rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
}

.shop-main .discount .stName{
  display: inline-block;
  font-size: 20rpx;
  color: #ffa333;
  border-radius: 24rpx;
  margin-right: 8rpx;
  border: 1rpx solid #ffa333;
  padding: 0 8rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
}

.shop-main .discount>text:first-child {
  color: #ff5a52;
  border: 1rpx solid #ff5a52;
}

.shop-price {
  position: relative;
}

.shop-price>text:first-child {
  font-size: 32rpx;
  color: #ff5a52;
  padding-right: 8rpx;
  font-weight: 500;
}

.shop-price>text:nth-child(2) {
  color: #a0a1a3;
  font-size: 24rpx;
  text-decoration: line-through;
  font-weight: 400;
  vertical-align: text-top;
}

.shop-price>image {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: 0;
  top: -10rpx;
  border-radius: 50%;
}

.reach-icon .resale{
  width: 20rpx;
  height: 20rpx;
  vertical-align: -2rpx;
}

.reachPrice{
  width: 100%;
  height: 32rpx;
  line-height: 32rpx;
  margin-bottom: 16rpx;
}

.reach-icon{
  display: inline-block;
  font-size: 20rpx;
  height: 28rpx;
  line-height: 28rpx;
  padding: 0 10rpx;
  border: 2rpx solid #FF5A52;
  border-radius: 4rpx;
  color: #FF5A52;
  font-weight: 400;
}

.purchase{
  display: inline-block;
  margin-left: 8rpx;
  padding: 0 10rpx;
  height: 28rpx;
  line-height: 28rpx;
  border: 2rpx solid #DCDEE0;
  color: #A0A1A3;
  font-size: 20rpx;
  border-radius: 4rpx;
  font-weight: 400;
}

/* 马上抢按钮 */
.immediately{
  height: 50rpx;
  padding: 0 10rpx;
  text-align: center;
  font-size: 26rpx;
  color: #fff;
  font-weight: 400;
  line-height: 50rpx;
  background-color: #FF5A52;
  border-radius: 24rpx;
  position: absolute;
  right: 0;
  top: 0;
}

.good_box {
  width: 14px;
  height: 14px;
  position: fixed;
  border-radius: 50%;
  overflow: hidden;
  left: 50%;
  top: 50%;
  z-index: +99;
  background: red;
}

