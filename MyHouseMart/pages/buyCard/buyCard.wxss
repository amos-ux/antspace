.buyCard{
  width: 100%;
  height: 100%;
  background: #F7F7F7;
  padding: 16px 40rpx 14px 40rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: relative;
}
.member{
  width: 670rpx;
  height: 290px;
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-radius: 6px;
  position: relative;
}
.memberImg{
  width: 311*2rpx;
  height: 163px;
}
.momberText{
  color: #292928;
  font-size: 20px;
  margin-bottom: 21px;
}
.momberState{
  color: #292928;
  font-size: 13px;
  margin-top: 14px;
}
.stuckType{
  position: absolute;
  top: 107px;
  left: 64rpx;
  color: #292928;
  font-size: 18px;
  font-weight: bold;
}
.stuckMoney{
  position: absolute;
  top: 104px;
  right: 72rpx;
  color: #292928;
  font-size: 16px;
  font-weight: bold;
}
.stuckMoney>text{
  font-size: 20px;
}
.validityPeriod{
  position: absolute;
  top: 140px;
  left: 64rpx;
  color: #946D29;
  font-size: 14px;
}
.experience{
  width: 690rpx;
  height: 92px;
  display: flex;
  flex-direction: row;
  background: url(https://antspace-prod-img-1.oss-cn-shenzhen.aliyuncs.com/16a35785d3b_group.svg) no-repeat;
  background-size:100%;
  margin-top: 13px;
  margin-left: 1rpx;
}
.free{
  width: 93%;
  height: 92px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-left: 48rpx;
}
.free text:nth-child(1){
  color: #292928;
  font-size: 16px;
  font-weight: bold;
}
.free text:nth-child(2){
  color: #5C5B5A;
  font-size: 12px;
}
.draw{
  width: 47%;
  height: 92px;
  display: flex;
  justify-content: center;
  align-items: center;
}
._draw{
  color: #F28100;
  font-size: 15px;
}
.draw_{
  color: #ADADAA;
  font-size: 15px;
}
.btn{
  width: 640rpx;
  height: 44px;
  background: #FFEB0D;
  color: #292928;
  font-size: 16px;
  font-weight: bold;
  text-align: center;
  line-height: 44px;
  margin-top: 132px;
}






