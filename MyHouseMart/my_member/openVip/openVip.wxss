page{
  width: 100%;
  height: 100%;
  position: relative;
  background-color: #fff;
}
.page {
  width: 100%;
  height: 4594rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.page image {
  width: 100%;
  height: 100%;
}

::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}

.big-box {
  width: 100%;
  height: 180rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
  z-index: 10;
  box-shadow: 2px -1px 1px 0px rgba(5, 114, 253, 0.05);
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}

.big-box view {
  width: 654rpx;
  height: 100rpx;
  background: linear-gradient(144deg, rgba(4, 194, 255, 1) 0%, rgba(4, 113, 253, 1) 100%);
  box-shadow: 0px 2px 8px 0px rgba(5, 114, 253, 0.24);
  border-radius: 50rpx;
  color: #fff;
  line-height: 100rpx;
  text-align: center;
  font-size: 50rpx;
  letter-spacing: 6rpx;
}

.vipInterests{
  width: 750rpx;
  height: 2630rpx;
  font-size: 0;
  position: relative;
 
}
.img1{
  width: 100%;
  height: 100%;
  position: relative;
  display: block;
}

.vipInterests .join{
  position: absolute;
  bottom: 60rpx;
  left: 0;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.join .join_btn{
  width:480rpx;
  height:92rpx;
  color: #F2CF9B;
  font-size: 34rpx;
  background:rgba(38,40,51,1);
  box-shadow:0 4rpx 16rpx 0 rgba(38,40,51,0.32);
  border-radius:48rpx;
  text-align: center;
  line-height: 92rpx;
}