  background-image: url("https://antspace-dev-img-1.oss-cn-shenzhen.aliyuncs.com/bb55c5f70e204d1f8dbdc1239175d089.png");
  background-size: 100% 100%;
  animation-name: move;
  /* 设置单次动画的执行时间 */
  animation-duration: 5s;
  /* 动画状态是否保留，设置为both可以保留动画结束时元素的样式 */
  animation-fill-mode: both;
  animation-timing-function: linear;
  animation-iteration-count: infinite;
}

@keyframes move {
  /* 0%表示动画开始的关键帧 */

  0% {
    top: 96rpx;
    left: 292rpx;
    transform: translate(0, 0);
    animation-timing-function: ease-in;
  }

  25% {
    top: 96rpx;
    left: 596rpx;
    animation-timing-function: ease-in;
  }

  50% {
    left: 596rpx;
    top: 366rpx;
    animation-timing-function: ease-in;
  }

  75% {
    left: 60rpx;
    top: 366rpx;
    animation-timing-function: ease-in;
  }

  90% {
    left: 60rpx;
    top: 760rpx;
    animation-timing-function: ease-in;
  }

  100% {
    left: 185rpx;
    top: 766rpx;
    animation-timing-function: ease-in;
  }/* 动画结束时的关键帧 */

  0% {
    transform: translate(0, 0);
    top: 96rpx;
    left: 292rpx;
  }
}

