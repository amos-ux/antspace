  -webkit-transform: skewx(-25deg);
  -moz-transform: skewx(-25deg);
  -o-transform: skewx(-25deg);
  -ms-transform: skewx(-25deg);
  transform: skewx(-25deg);
}

@keyframes guangshumove {
  /* 0%表示动画开始的关键帧 */

  0% {
    left: 0%;
    transition: left 1s ease 0s;
  }

  100% {
    left: 150%;
    transition: left 1s ease 0s;
  }/* 动画结束时的关键帧 */

  0% {
    left: 0%;
    transition: left 1s ease 0s;
  }
}

