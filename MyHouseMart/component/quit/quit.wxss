.page{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.8);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  z-index: 9999
}
.page-content{
    height: 342rpx;
    width: 580rpx;
    background: #fff;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    border-radius: 12rpx;
  
}
.page-title{
  color: #2B2D33;
  font-size: 36rpx;
  margin-top: 48rpx;
  font-weight: 500
}
.page-subtitle{
  color: #67686B;
  padding: 0 40rpx;
  font-size: 32rpx;
  text-align: center
}
.navgitor{
    width: 580rpx;
    height: 100rpx;
    text-align: center;
    line-height: 100rpx;
    color: #FFA333;
    font-size: 36rpx;
    border-top: 1rpx solid #EBEBEB;
}